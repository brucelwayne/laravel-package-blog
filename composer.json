{
    "name": "brucelwayne/laravel-package-blog",
    "description": "An open-source blog system based on Laravel that is ready to use out of the box.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruce Wayne"
        }
    ],
    "require": {
        "unisharp/laravel-filemanager": "^v2.6",
        "spatie/laravel-sluggable": "^3.5",
        "rinvex/laravel-categories": "^7.1",
        "spatie/laravel-translatable": "^6.5",
        "spatie/laravel-tags": "^4.5",
        "spatie/laravel-medialibrary": "^10.10.1",
        "mcamara/laravel-localization": "^1.8",
        "ausi/slug-generator": "^1.1",
        "spatie/laravel-permission": "^5.9",
        "jimmiw/php-time-ago": "^3.2",
        "laravel/scout": "^10.0",
        "mtownsend/read-time": "^2.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Brucelwayne\\Blog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Brucelwayne\\Blog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brucelwayne\\Blog\\BlogServiceProvider"
            ],
            "aliases": {
                "Blog": "Brucelwayne\\Blog\\Facades\\BlogFacade"
            }
        }
    }
}